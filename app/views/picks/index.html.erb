<h1>Listing picks</h1>

<table>
  <tr>
    <th>Picker</th>
    <th>Picked</th>
    <th>Hash</th>
    <th>Has checked</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @picks.each do |pick| %>
  <tr>
    <td><%= pick.picker_id %></td>
    <td><%= pick.picked_id %></td>
    <td><%= pick.hash %></td>
    <td><%= pick.has_checked %></td>
    <td><%= link_to 'Show', pick %></td>
    <td><%= link_to 'Edit', edit_pick_path(pick) %></td>
    <td><%= link_to 'Destroy', pick, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pick', new_pick_path %>
